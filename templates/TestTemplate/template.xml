<?xml version="1.0" ?>

<PhotoTemplate 
	xmlns="http://www.scottmckittrick.com/schema/PiBooth/PhotoTemplate"
	>
  <name>Test Template</name>
  <description>Test template for python photo booth</description>
  <!-- Preview Image-->
  <previewImage src="preview.png" />

  <canvas height="1080" width="1920" backgroundColor="#FF00FF">
    <backgroundPhoto src="background.jpg" />

    <foregroundPhoto src="overlay.png" />

    <photos>
      <photoSpec height="300" width="400" x="30" y="30" rotation="0"/>
      <photoSpec height="300" width="400" x="460" y="30" />
      <photoSpec height="300" width="400" x="800" y="500" rotation="90"/>
    </photos>
  </canvas>

</PhotoTemplate>
